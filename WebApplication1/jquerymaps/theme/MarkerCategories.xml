<?xml version="1.0" encoding="UTF-8"?>
<jqm_markerCategories xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/jqm_markerCategories.xsd">
